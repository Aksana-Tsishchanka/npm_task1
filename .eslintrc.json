{
  "parser": "babel-eslint",
  "rules": {
    "semi": 2,
    "indent": ["error", 2],
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "object-curly-spacing": ["error", "always", { "objectsInObjects": false }],
    "no-console": 1
  },
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true
  }
}